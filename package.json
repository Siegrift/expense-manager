{
  "name": "expense-manager",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "deploy": "yarn build && vercel --prod",
    "dev": "ENVIRONMENT=development NODE_OPTIONS=--max_old_space_size=8192 next dev",
    "lint:fix": "yarn lint --fix",
    "lint": "yarn eslint --ext js,ts,tsx . --max-warnings 0",
    "prettier:fix:quick": "pretty-quick --staged",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier": "prettier --check \"./**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run",
    "test:e2e:start-server": "ENVIRONMENT=staging next dev -p 3000",
    "test:e2e": "start-server-and-test \"yarn test:e2e:start-server\" http://localhost:3000 \"yarn test:e2e:run\"",
    "test:unit:watch": "yarn test:unit --watch",
    "test:unit": "jest --runInBand",
    "test": "yarn typescript && yarn test:unit && yarn test:e2e",
    "typescript:watch": "tsc -p . --watch",
    "typescript": "tsc -p .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^4.0.0-alpha.11",
    "@monaco-editor/react": "^4.3.1",
    "@nivo/core": "^0.74.0",
    "@nivo/line": "^0.74.0",
    "@nivo/pie": "^0.74.0",
    "@nivo/sunburst": "^0.74.0",
    "@siegrift/tsfunct": "^1.6.0",
    "@types/js-beautify": "^1.13.3",
    "@visx/responsive": "^2.4.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "firebase": "^7.24.0",
    "firebase-admin": "^9.3.0",
    "js-beautify": "^1.14.0",
    "lodash": "^4.17.21",
    "material-ui-dropzone": "^3.5.0",
    "mathjs-expression-parser": "^1.0.2",
    "next": "^12.0.4",
    "next-offline": "^5.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-button": "^0.7.2",
    "react-highlight.js": "^1.0.7",
    "react-icons": "^4.3.1",
    "react-number-format": "^4.8.0",
    "react-redux": "^7.2.6",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.0",
    "reselect": "^4.1.4",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.4",
    "@testing-library/cypress": "^8.0.2",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.177",
    "@types/next": "^9.0.0",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-highlight.js": "^1.0.2",
    "@types/react-redux": "^7.1.20",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.0.0",
    "cypress-firebase": "^2.0.0",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-testing-library": "^4.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-react": "^7.27.0",
    "firebase-mock": "^2.3.2",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  }
}
