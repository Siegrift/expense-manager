{
  "name": "expense-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "deploy": "yarn build && now --prod",
    "dev": "NODE_OPTIONS=--max_old_space_size=8192 next dev",
    "check-eslint": "eslint --print-config pages/index.js | eslint-config-prettier-check",
    "run-eslint": "eslint \"lib/**\" \"pages/**\" \"cypress/integration/**\"",
    "lint": "yarn check-eslint && prettier --check \"@(lib|pages)/**/*.tsx\" && yarn run-eslint",
    "lint:fix": "prettier --write \"@(lib|pages)/**/*.tsx\" \"@(lib|pages)/**/*.ts\" && yarn run-eslint --fix",
    "test": "yarn typescript && yarn test:unit && yarn test:e2e",
    "typescript": "tsc -p .",
    "test:unit": "jest",
    "test:unit-watch": "jest --watch",
    "test:e2e-start-server": "next dev -p 3000",
    "test:e2e-run": "cypress run",
    "test:e2e-open": "cypress open",
    "test:e2e": "start-server-and-test 'yarn test:e2e-start-server' http://localhost:3000 'yarn test:e2e-run'",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged && yarn typescript",
      "pre-push": "yarn test"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "endOfLine": "lf"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "4.0.0-alpha.11",
    "@nivo/line": "^0.63.1",
    "@nivo/pie": "^0.63.1",
    "@nivo/sunburst": "^0.63.1",
    "@siegrift/tsfunct": "^1.6.0",
    "@visx/responsive": "^1.1.0",
    "@zeit/next-css": "^1.0.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "firebase": "^7.24.0",
    "firebase-admin": "^9.3.0",
    "js-beautify": "^1.13.4",
    "lodash": "^4.17.20",
    "material-ui-dropzone": "^3.5.0",
    "mathjs-expression-parser": "^1.0.2",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "next": "9.5.5",
    "next-offline": "5.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-google-button": "^0.7.1",
    "react-highlight.js": "^1.0.7",
    "react-icons": "^3.11.0",
    "react-monaco-editor": "^0.40.0",
    "react-number-format": "^4.4.1",
    "react-redux": "^7.2.1",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.5.5",
    "@testing-library/cypress": "^7.0.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.162",
    "@types/next": "^9.0.0",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-highlight.js": "^1.0.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-select": "^3.0.22",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@types/redux-logger": "^3.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "cross-env": "^7.0.2",
    "cypress-firebase": "^1.5.0",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-testing-library": "^4.0.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "firebase-mock": "^2.3.2",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.4.2",
    "prettier": "2.1.2",
    "start-server-and-test": "^1.11.5",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3"
  }
}
